<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Authors>Miroslav Bartl</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Allows dynamical creation of factory classes inside IServiceCollection, similar to Ninject's ToFactory functionallity.</Description>
    <Version>0.1.0</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/czmirek/IServiceCollection.AddFactory-extension</PackageProjectUrl>
    <RepositoryUrl>https://github.com/czmirek/IServiceCollection.AddFactory-extension</RepositoryUrl>
    <PackageTags>IoC, DI, Factory</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.9" />
  </ItemGroup>

</Project>
